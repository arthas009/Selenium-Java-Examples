<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Default Suite">
    <listeners>
        <listener class-name="org.testng.reporters.EmailableReporter"></listener>
    </listeners>

    <test thread-count="1" name="Search_Tests">
        <classes>
            <class name="Functionality_SearchTests"/>
        </classes>
    </test>
    <test thread-count="1" name="Login_Tests">
        <classes>
            <class name="Functionality_LoginTests"/>
        </classes>
    </test>
    <test thread-count="1" name="Register_Tests">
        <classes>
            <class name="Functionality_RegisterTests"/>
        </classes>
    </test>

</suite> <!-- Default Suite -->
